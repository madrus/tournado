{
  "localesPaths": ["app/i18n/locales"],
  "keystyle": "nested",
  "sourceLanguage": "nl",
  "enabledFrameworks": ["react-i18next"],
  "namespace": true,
  "defaultNamespace": "root",
  "pathMatcher": "{locale}.json",
  "sortKeys": true,
  "keepFulfilled": true,
  "preferredDelimiter": ".",
  "projectType": "react-i18next",
  "enabledParsers": ["json"],
  "extract": {
    "autoDetect": {
      "sourceLanguage": "nl",
      "targetLanguages": ["en", "ar", "de", "fr", "tr"]
    }
  }
}
