{
	"printWidth": 88,
	"useTabs": true,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"trailingComma": "all",
	"semi": false,
	"bracketSpacing": true,
	"arrowParens": "always",
	"endOfLine": "lf",
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": ["<THIRD_PARTY_MODULES>", "^~/(.*)$", "^[./]"],
	"importOrderSeparation": false,
	"importOrderSortSpecifiers": true
}
