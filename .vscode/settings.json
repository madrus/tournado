{
  "json.schemaDownload.enable": true,
  "json.schemaDownload.timeout": 10000,
  "json.schemaDownload.retry": 3,
  "json.schemaDownload.cache": true,
  "i18n-ally.localesPaths": ["app/i18n/locales"],
  "i18n-ally.sourceLanguage": "en",
  "i18n-ally.enabledFrameworks": ["react", "i18next"],
  "i18n-ally.keystyle": "flat", // or "flat" if your JSON is flat
  "i18n-ally.sortKeys": true,
  "i18n-ally.pathMatcher": "{locale}.json",
  "i18n-ally.keepFulfilled": true,
  "i18n-ally.preferredDelimiter": ".",
  "i18n-ally.projectType": "react-i18next",
  "i18n-ally.enabledParsers": ["json"],
  "i18n-ally.extract.autoDetect.sourceLanguage": "en",
  "i18n-ally.extract.autoDetect.targetLanguages": ["nl", "fr", "ar", "tr"],
  "i18n-ally.extract.autoDetect.enabled": true
}
